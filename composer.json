{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "illuminate/auth": "^6.0.2",
        "illuminate/broadcasting": "^6.0.2",
        "illuminate/bus": "^6.0.2",
        "illuminate/cache": "^6.0.2",
        "illuminate/config": "^6.0.2",
        "illuminate/console": "^6.0.2",
        "illuminate/container": "^6.0.2",
        "illuminate/contracts": "^6.0.2",
        "illuminate/database": "^6.0.2",
        "illuminate/encryption": "^6.0.2",
        "illuminate/events": "^6.0.2",
        "illuminate/filesystem": "^6.0.2",
        "illuminate/hashing": "^6.0.2",
        "illuminate/http": "^6.0.2",
        "illuminate/log": "^6.0.2",
        "illuminate/pagination": "^6.0.2",
        "illuminate/pipeline": "^6.0.2",
        "illuminate/queue": "^6.0.2",
        "illuminate/session": "^6.0.2",
        "illuminate/support": "^6.0.2",
        "illuminate/translation": "^6.0.2",
        "illuminate/validation": "^6.0.2",
        "laravel/lumen-framework": "^6.0",
        "laravel/socialite": "^4.2"
    },
    "require-dev": {
		"php": "^7.2",
        "laravel/lumen-framework": "^6.0",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
		"metrogistics/laravel-azure-ad-oauth":"master",
		"illuminate/auth": "^6.0.2",
		"illuminate/contracts": "^6.0.2",
		"illuminate/http": "^6.0.2",
		"illuminate/queue": "^6.0.2",
		"illuminate/support": "^6.0.2",
		"illuminate/session": "^6.0.2",
		"illuminate/console": "^6.0.2",
		"illuminate/container": "^6.0.2",
		"illuminate/database": "^6.0.2",
		"illuminate/filesystem": "^6.0.2",
		"illuminate/pipeline": "^6.0.2",
		"illuminate/broadcasting": "^6.0.2",
		"illuminate/bus": "^6.0.2",
		"illuminate/cache": "^6.0.2",
		"illuminate/config": "^6.0.2",
		"illuminate/encryption": "^6.0.2",
		"illuminate/events": "^6.0.2",
		"illuminate/hashing": "^6.0.2",
		"illuminate/log": "^6.0.2",
		"illuminate/pagination": "^6.0.2",
		"illuminate/translation": "^6.0.2",
		"illuminate/validation": "^6.0.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
